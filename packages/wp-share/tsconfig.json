{
  "compilerOptions": {
      "module": "commonjs",
      "esModuleInterop": true,
      "allowSyntheticDefaultImports": true,
      // "target": "es6",
      "target": "esnext",
      // "module": "esnext",
      "declaration":true , 
      "declarationDir": "cjs",
      "noImplicitAny": true,
      "moduleResolution": "node",
      "sourceMap": false ,
      "outDir": "cjs",
      "baseUrl": ".",
      "paths": {
          "*": [
              "node_modules/*",
              "src/types/*"
          ]
      }
  },
  "include": [
      "src/**/*"
  ]
}
